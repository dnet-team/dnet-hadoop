@namespace("eu.dnetlib.dhp.importer.schemas")
protocol DHP {

  enum RecordFormat {
    XML, JSON
  }

  record ImportedRecord {

    // record identifier
    string id;

    RecordFormat format;

    // format name (OAF, OAI_DC, Datacite, etc) for which there is a parser implementation
    string formatName;

    // record body
    string body;
  }
}
